{
  "version": 2,
  "buildCommand": "vite build",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "zeroConfig": true,
        "installCommand": "npm install --force"
      }
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "env": {
    "SKIP_VERSION_VALIDATION": "true",
    "NODE_ENV": "production"
  }
}
